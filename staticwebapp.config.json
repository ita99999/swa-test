          "registration": {
            "openIdIssuer": "https://login.microsoftonline.com/943a464b-de80-4ed2-b453-45c4cce25889/v2.0",
            "clientIdSettingName": "PROVIDER_CLIENT_ID",
            "clientSecretSettingName": "PROVIDER_CLIENT_SECRET"
          }
        }
      }
    }
  }